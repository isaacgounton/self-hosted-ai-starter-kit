POSTGRES_USER=root
POSTGRES_PASSWORD=your-secure-postgres-password-here
POSTGRES_DB=n8n

N8N_ENCRYPTION_KEY=your-secure-encryption-key-here
N8N_USER_MANAGEMENT_JWT_SECRET=your-secure-jwt-secret-here
N8N_DEFAULT_BINARY_DATA_MODE=filesystem
SERVICE_URL_N8N=http://localhost:5678

# Task Runners Configuration (for Python support)
N8N_RUNNERS_AUTH_TOKEN=your-secure-runners-token-here

# MinIO Configuration
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=your-secure-minio-password-here

# VPS Deployment Configuration (for automated deployment)
VPS_HOST=your-vps-ip-here
VPS_SSH_USER=root
VPS_SSH_KEY_PATH=~/.ssh/your-vps-key-here

# Ollama Configuration (currently deactivated)
